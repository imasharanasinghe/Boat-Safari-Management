# .gitignore for Android projects

# Built application files
*.apk
*.ap_
*.aab

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/
build/

# Gradle files
.gradle/
build/
!gradle/wrapper/gradle-wrapper.jar

# Local configuration file (sdk path, etc)
local.properties

# Proguard folder generated by Eclipse
proguard/

# Log files
*.log

# Android Studio
.idea/
*.iml
*.ipr
*.iws
*.hprof

# Keystore files
*.jks
*.keystore

# NDK
obj/

# Mac OS
.DS_Store

# Windows
Thumbs.db

# Others
captures/
.externalNativeBuild/
.cxx/ 